    <script>
      $(document).ready(function() {
        var tl = gsap.timeline();

        tl.to(".card-2", {
            top: "30%",
            opacity: 1,
            duration: 1.5
          })
          .to(".card-1", {
            top: "20%",
            opacity: 1,
            duration: 1.5
          }, "=0.5")
          .to(".popbtn", {
            scale: 1.05,
            duration: .2
          })
          .to(".popbtn", {
            scale: 1,
            backgroundColor: "green",
            duration: 0.2
          })
          .to(".card-3", {
            top: "50%",
            opacity: 1,
            duration: 1.5
          }, "=0.5")
          .to(".pm2-progress, .pm2-progress2", {
            width: "65%",
            duration: 0.4
          })
          .to(".pm2-progress2", {
            width: "25%",
            duration: 0.4
          })
          .to(".rotation", {
            rotation: 180,
            duration: .4
          })
          .to(".pop", {
            scale: 1.3,
            duration: .4
          })
          .to(".pop", {
            scale: 1,
            duration: 0.1
          })
          .to(".card-4", {
            top: "20%",
            opacity: 1,
            duration: 1
          }, "=0.1");
      });
    </script>


